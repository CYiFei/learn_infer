#include <iostream>
#include <armadillo>

using namespace std;
using namespace arma;

int main()
{
    // 定义矩阵 A, B, C
    mat A = {{1, 2}, {3, 4}};
    mat B = {{5, 6}, {7, 8}};
    mat C = {{1, 1}, {1, 1}};

    // 计算 A * B 并加上矩阵 C
    mat result = A * B + C;

    // 输出结果
    cout << "Result:\n" << result << endl;

    return 0;
}